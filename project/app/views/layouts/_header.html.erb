	<div id="header" class="fullwidth">
		<div class="fixedwidth">
				eskolarship
			<span id="nav-list" class="right">
				<% if current_user %>
					<% if current_user.tag == 0 %>
						<%= link_to 'Home', applicants_path,
						method: :get, class: "nav-item left" %>
						<%= link_to 'Profile', profile_applicants_path, method: :get %>
					<% else %>
						<%= link_to 'Home', sponsors_path, method: :get, class: "nav-item left" %>
						<%= link_to 'Profile', profile_sponsors_path, method: :get %>
					<% end %>
					<%= link_to 'Logout', logout_accounts_path, method: :post %>
				<% else %>
					<%= link_to 'Home', root_path,
						method: :get, class: "nav-item left" %>
					<%= link_to 'Login', login_accounts_path,
						method: :get, class: "nav-item left" %>
					<%= link_to 'Sign Up', new_applicant_path(:tag=>0),
						method: :get, class: "nav-item left" %>
					<%= link_to 'Be A Sponsor', new_sponsor_path(:tag=>1),
						method: :get, class: "nav-item left" %>
				<% end %>
			</span>
		</div>
	</div>